{
    "name": "dansan/joboy",
    "description": "It's a little jobman... a little job manager",
    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "Danilo Sanchi",
            "email": "danilo.sanchi@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "ext-redis": "*",
        "ramsey/uuid": "^3.8",
        "symfony/finder": "*",
        "symfony/property-access": "*",
        "symfony/proxy-manager-bridge": "*",
        "symfony/stopwatch": "^4.2",
        "symfony/yaml": "*"
    },
    "require-dev": {
        "dansan/fixture-handler": "*",
        "mockery/mockery": "dev-master",
        "symfony/console": "^3.4",
        "symfony/dotenv": "^4.2",
        "symfony/phpunit-bridge": "*",
        "symfony/var-dumper": "^4.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "process-timeout": 60,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Jobboy\\": "src/Jobboy/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.x-dev"
        }
    },

    "minimum-stability": "stable"
}
