{
    "name": "dansan/jobboy",
    "description": "It's a little jobman... a little job manager",
    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "Danilo Sanchi",
            "email": "danilo.sanchi@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "beberlei/assert": "^3.0",
        "ramsey/uuid": "^3.8",
        "symfony/lock": "^4.3",
    },
    "require-dev": {
        "ext-json": "*",
        "ext-redis": "*",
        "doctrine/dbal": "~2.4",
        "nesbot/carbon": "^2.0",
        "dansan/fixture-handler": "*",
        "mockery/mockery": "dev-master",
        "symfony/console": "^3.4",
        "symfony/dotenv": "^4.2",
        "symfony/phpunit-bridge": "*",
        "symfony/var-dumper": "^4.2",
        "symfony/framework-bundle": "^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "process-timeout": 60,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "JobBoy\\": "src/JobBoy/",
            "Dan\\": "src/Dan/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.x-dev"
        }
    },

    "minimum-stability": "stable"
}
